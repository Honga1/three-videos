{
  "name": "three-videos",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-http-proxy": "^1.6.2",
    "express-sslify": "^1.2.0",
    "request": "^2.88.2"
  },
  "scripts": {
    "watch:server-js": "cross-env NODE_ENV=development PWD=./ nodemon ./backend/dist/app.js",
    "watch:server-ts": "cd ./backend && cross-env NODE_ENV=development PWD=./ tsc -w",
    "server": "concurrently \"npm:watch:server-js\" \"npm:watch:server-ts\"",
    "start:dev": "concurrently \"npm:server\" \"npm:frontend\"",
    "start": "NODE_ENV=production node ./backend/dist/app.js",
    "frontend": "react-scripts start",
    "build:frontend": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:server": "cd ./backend && tsc",
    "build": "npm run build:server && npm run build:frontend",
    "eslint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\" \"./backend/src/**/*.ts\"",
    "lint": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "deploy": ".deploy/deploy.sh"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cors": "^2.8.8",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/express": "^4.17.9",
    "@types/express-http-proxy": "^1.6.1",
    "@types/express-sslify": "^1.2.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "^4.0.1",
    "typescript": "^4.1.2",
    "web-vitals": "^0.2.4",
    "zustand": "^3.2.0"
  }
}
